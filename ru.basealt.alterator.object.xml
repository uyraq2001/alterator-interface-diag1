<!DOCTYPE node PUBLIC
"-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">

  <interface name="ru.basealt.alterator.object">

    <doc:doc>
      <doc:description>
        <doc:para>
          Overview of D-Bus object.
        </doc:para>
      </doc:description>
    </doc:doc>

    <!-- methods -->

    <method name="info">
      <arg name="stdout_bytes" direction="out" type="ay">
        <doc:doc><doc:summary>Byte array containing .desktop file.</doc:summary></doc:doc>
      </arg>

	  <arg name="response" direction="out" type="i">
        <doc:doc><doc:summary>Byte array containing .desktop file.</doc:summary></doc:doc>
      </arg>


      <doc:doc>
        <doc:description>
          <doc:para>
            Information about object in .desktop format.
	  </doc:para>
        </doc:description>
      </doc:doc>
    </method>
  </interface>

</node>
